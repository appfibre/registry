{
    "$schema": "http://registry.appfibre.org/schema/config.json#20",
    "plugins": {
          "$": {
              "processors": {
                "default": [
                    { "src": "http://cdnjs.appfibre.org/bundles/cjs/plugins-server.js#core", "module": "CommonJS" }
                    , { "src": "http://cdnjs.appfibre.org/bundles/es/plugins-server.js#core", "module": "ES" } 
                ]
            }
          }
        , "$server": {
            "processors": {
                "default": [{ "src": "http://cdnjs.appfibre.org/bundles/cjs/plugins-server.js#server", "module": "CommonJS" }]
            }
        }
    },
    "settings": {
        "transform": "application/jst"
    }
}