{
    "$schema": "http://registry.appfibre.org/schema/config.json#19",
    "plugins": {
          "$": {
              "processors": {
                "default": [
                    { "src": "http://cdnjs.appfibre.org/cjs/plugins-server.js#core", "module": "CommonJS" }
                    , { "src": "http://cdnjs.appfibre.org/es/plugins-server.js#core", "module": "ES" } 
                ]
            }
          }
        , "$server": {
            "processors": {
                "default": [{ "src": "http://cdnjs.appfibre.org/cjs/plugins-server.js#server", "module": "CommonJS" }]
            }
        }
        , "$mssql": {
            "processors": {
                "default": [
                    { "src": "http://cdnjs.appfibre.org/cjs/plugins-mssql.js"
                    , "module": "CommonJS" 
                    , "requifres": ["events", "punycode", "string_decoder", "stream", "tty", "os", "url", "buffer", "crypto", "tls", "http", "htps", "assert", "zlib", "path", "fs", "net", "timers", "querystring", "child_process", "dgram", "dns", "util"
                        , "readable-stream", "xmlbuilder", "sshpk"]
                    }
                ]
            }
        }
    },
    "settings": {
        "transform": "application/jst"
    }
    

}