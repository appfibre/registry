{
    "$schema": "https://registry.appfibre.org/schema/config.json",
    "plugins": {
          "$": {
              "schema": "TODO $",
              "processors": {
              "default": [
                  { "src": "https://unpkg.com/@appfibre/bundles/plugins/core.js", "module": "IIFE" }
                , { "src": "https://unpkg.com/@appfibre/bundles/plugins/core.umd.js", "module": "UMD" }
                , { "src": "https://unpkg.com/@appfibre/bundles/plugins/core.cjs.js", "module": "CommonJS" }
                ]
            },
            "tests": [ { "src": "https://unpkg.com/@appfibre/bundles/plugins/tests/core.spec.json"}
                     , { "src": "https://unpkg.com/@appfibre/bundles/plugins/tests/html.spec.json"} ]
        }
    }, "settings": {
        "transform": ["application/jst", "application/json"]
    }
}